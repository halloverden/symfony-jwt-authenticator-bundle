{
    "name": "halloverden/symfony-jwt-authenticator-bundle",
    "description": "JWT authenticator for Symfony",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "HalloVerden\\JwtAuthenticatorBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HalloVerden\\JwtAuthenticatorBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Tom Roar Furunes",
            "email": "tom.roar@halloverden.no"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "symfony/security-bundle": "^6.4|^7.0",
        "web-token/jwt-bundle": "^3.0",
        "web-token/jwt-library": "^3.0",
        "symfony/yaml": "^6.4|^7.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^6.4|^6.0"
    }
}
